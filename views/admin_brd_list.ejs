<% if(!userData){ %>
    <script>
        alert("관리자만 접근할 수 있는 페이지 입니다.")
        location.href = "/admin";
    </script>
<% } %>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>관리자 게시판</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <%- include("common/admin_header.ejs") %>
    <div id="board_list_wrap">
        <div class="center">
            <table class="brd_wrap">
                <tr class="brd_top">
                    <th class="number">번호</th>
                    <th class="auther">분류</th>
                    <th >제목</th>
                    <th class="date">날짜</th>
                </tr>
                <!-- 반복문으로 데이터 받아와서 여기에 넣어주기 -->
                <% brdData.forEach(function(el, index){ %>
                    <tr class="brd_bot">
                        <td class="number"><%- el.num %></td>
                        <td class="classification"><%- el.classification %></td>
                        <td class="title user_title"><a href="/admin/detail/<%- el.num %>"><%- el.title %></a></td>
                        <td class="date user_date"><%- el.date %></td>
                    </tr>
                <% }); %>
            </table>
            <a class="add_btn" href="/admin/board_insert">게시글 작성</a>
        </div>
    </div>
</body>
</html>